<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.loadx.application.db.entity.LoadTask" table="LOAD_TASK">

        <meta attribute = "class-description">
            Load task representation.
        </meta>

        <id name="id" type="integer" column="id">
            <generator class="native"/>
        </id>

        <property name="baseUrl" column="baseUrl" type="java.lang.String"/>
        <property name="loadingTime" column="loadingTime" type="integer"/>
        <property name="parallelThreshold" column="parallelThreshold" type="integer"/>

    </class>
</hibernate-mapping>